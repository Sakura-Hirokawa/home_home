<div class = "form-inline mb-3">
  <%= form_with url: search_path, method: :get, local:true do |f| %>
    <%= f.text_field :content, class: "form-control col-6 mr-3" %>
    <%= f.select :model, options_for_select({"User" => "user"}) %>
    <%= f.select :method, options_for_select({"完全一致" => "perfect", "前方一致" => "forward", "後方一致" => "backward", "部分一致" => "partial"}) %>
    <%= f.submit "検索", class: "btn btn-primary ml-3" %>
  <% end %>
</div>